{
  "compilerOptions": {
    "alwaysStrict": true,
    "moduleResolution": "node",
    "locale": "zh-CN",
    "module": "ESNext",
    "target": "ES5",
    "newLine": "LF",
    "strictNullChecks": true,
    "declaration": true,
    "lib": [
      "ES2015",
      "ES2016",
      "ES2017",
      "ES2018",
      "DOM"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}